{
  "name": "blogr-landing-page",
  "version": "1.0.0",
  "description": "challenge from frontendmentor",
  "scripts": {
    "start": "parcel ./public/index.html --port 3000 --dist-dir ./.dev --no-cache",
    "build": "parcel build ./public/index.html --dist-dir ./build --no-cache --public-url ./"
  },
  "keywords": [],
  "author": "claude",
  "license": "MIT",
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.5.0",
    "@parcel/transformer-typescript-tsc": "^2.5.0",
    "@types/react": "^18.0.7",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "parcel": "^2.5.0",
    "parcel-namer-struct": "^1.0.0",
    "postcss": "^8.4.12",
    "postcss-font-magician": "^3.0.0",
    "styled-components": "^5.3.5",
    "typescript": "^4.6.3"
  },
  "parcel-namer-struct": [
    {
      "match": ".(svg|png|jpe?g)$",
      "dest": "/images/"
    },
    {
      "match": ".tsx$",
      "dest": "/js/"
    },
    {
      "match": ".sass$",
      "dest": "/css/"
    }
  ]
}
